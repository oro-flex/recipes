{
    "manifests": {
        "oro-flex/cache-bundle": {
            "manifest": {
                "bundles": {
                    "Oro\\Bundle\\CacheBundle\\OroCacheBundle": [
                        "prod"
                    ]
                },
                "copy-from-recipe": {
                    "config": "%CONFIG_DIR%/"
                },
                "env": {
                    "WEB_SOCKET_PORT": "6666"
                },
                "post-install-output": [
                    "<bg=blue;fg=white>                     </>",
                    "<bg=blue;fg=white> Next: Configuration </>",
                    "<bg=blue;fg=white>                     </>",
                    "  * Configure your parameters in <fg=green>config/packages/oro_config.yaml</>"
                ]
            },
            "files": {
                "config\\packages\\oro_cache.yaml": {
                    "contents": [
                        "framework:",
                        "    cache:",
                        "        pools:",
                        "            cache.annotations:",
                        "                adapter: cache.adapter.doctrine",
                        "                provider: oro_cache.annotations",
                        "            cache.serializer:",
                        "                adapter: cache.adapter.doctrine",
                        "                provider: oro_cache.serializer",
                        "            cache.validator:",
                        "                adapter: cache.adapter.doctrine",
                        "                provider: oro_cache.validator",
                        "            oro.data.cache:",
                        "                adapters:",
                        "                    - oro.cache.provider.memory_cache.cache",
                        "                    - oro_cache.data.adapter",
                        "            oro.data.cache.without_memory_cache:",
                        "                adapter: oro_cache.data.adapter",
                        "#",
                        "#    annotations:",
                        "#        cache: php_array",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "803496eb0afd88140df5341411840b93bf0e9e65"
        }
    }
}
